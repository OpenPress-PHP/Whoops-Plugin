{
    "name": "openpress/whoops",
    "description": "Adds Whoops to OpenPress",
    "version": "0.1.0",
    "type": "openpress-plugin",
    "authors": [
        {
            "name": "Dominic Carrington",
            "email": "carringtondominic@gmail.com"
        }
    ],
    "repositories": [
        {
            "type": "vcs",
            "url": "https://github.com/OpenPress-PHP/Custom-Installer"
        }
    ],
    "require": {
        "openpress/composer-custom-installer": "dev-master",
        "filp/whoops": "^2.2"
    },
    "autoload": {
        "psr-4": {
            "OpenPress\\Plugin\\Whoops\\": "src/"
        }
    },
    "extra": {
        "openpress": {
            "class": "OpenPress\\Plugin\\Whoops\\WhoopsPlugin",
            "priority": 100
        }
    }
}
